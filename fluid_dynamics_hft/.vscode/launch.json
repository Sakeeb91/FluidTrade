{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Fluid Market System",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/fluid_market_system.py",
            "args": [
                "--ticker", "AAPL",
                "--date", "2023-01-01",
                "--synthetic"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Run All Tests",
            "type": "python",
            "request": "launch",
            "module": "unittest",
            "args": [
                "discover",
                "-s",
                "tests",
                "-p",
                "test_*.py"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Run Strategy Tests",
            "type": "python",
            "request": "launch",
            "module": "unittest",
            "args": [
                "tests.test_fluid_hft_strategy"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Run Model Tests",
            "type": "python",
            "request": "launch",
            "module": "unittest",
