    "class FluidTradePortfolioManager:\n",
    "    \"\"\"\n",
    "    Advanced portfolio management with multiple strategies and risk controls.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, initial_capital=100000, risk_budget=0.02):\n",
    "        self.initial_capital = initial_capital\n",
    "        self.risk_budget = risk_budget  # Daily VaR limit\n",
    "        \n",
    "        # Portfolio state\n",
    "        self.strategies = {}\n",
    "        self.allocations = {}  # Strategy allocations\n",
    "        self.risk_metrics = []\n",
    "        \n",
    "    def add_strategy(self, name, strategy, allocation):\n",
    "        \"\"\"\n",
    "        Add a strategy to the portfolio.\n",
    "        \"\"\"\n",
    "        self.strategies[name] = strategy\n",
    "        self.allocations[name] = allocation\n",
    "        \n",
    "        print(f\"=Ê Added strategy '{name}' with {allocation*100:.1f}% allocation\")\n",
    "    \n",
    "    def calculate_portfolio_var(self, returns, confidence_level=0.05):\n",
    "        \"\"\"\n",
    "        Calculate Value at Risk for the portfolio.\n",
    "        \"\"\"\n",
    "        if len(returns) < 30:\n",
    "            return 0\n",
    "        \n",
    "        # Historical VaR\n",
    "        var = np.percentile(returns, confidence_level * 100)\n",
    "        \n",
    "        # Expected Shortfall (Conditional VaR)\n",
    "        es = returns[returns <= var].mean()\n",
    "        \n",
    "        return var, es\n",
    "    \n",
    "    def dynamic_allocation(self, market_state):\n",
    "        \"\"\"\n",
    "        Dynamically adjust strategy allocations based on market conditions.\n",
    "        \"\"\"\n",
    "        # Adjust based on turbulence\n",
    "        turbulence = market_state.get('turbulence', 0.3)\n",
    "        \n",
    "        # Reduce overall exposure in high turbulence\n",
    "        turbulence_factor = max(0.3, 1 - turbulence * 2)\n",
    "        \n",
    "        # Adjust allocations\n",
    "        adjusted_allocations = {}\n",
    "        total_allocation = 0\n",
    "        \n",
    "        for strategy_name, base_allocation in self.allocations.items():\n",
    "            adjusted = base_allocation * turbulence_factor\n",
    "            adjusted_allocations[strategy_name] = adjusted\n",
    "            total_allocation += adjusted\n",
    "        \n",
    "        # Normalize to ensure allocations sum to <= 1\n",
    "        if total_allocation > 1:\n",
    "            for strategy_name in adjusted_allocations:\n",
    "                adjusted_allocations[strategy_name] /= total_allocation\n",
    "        \n",
    "        return adjusted_allocations\n",
    "    \n",
    "    def run_portfolio_backtest(self, data):\n",
    "        \"\"\"\n",
    "        Run backtest with multiple strategies and dynamic allocation.\n",
    "        \"\"\"\n",
    "        print(f\"<Æ Running portfolio backtest with {len(self.strategies)} strategies...\")\n",
    "        \n",
    "        # Generate signals for each strategy\n",
    "        strategy_results = {}\n",
    "        \n",
    "        for name, strategy in self.strategies.items():\n",
    "            print(f\"  =Ê Generating signals for {name}...\")\n",
    "            \n",
    "            signals, confidence = strategy.generate_combined_signals(data)\n",
    "            \n",
    "            # Store strategy data\n",
    "            strategy_data = data.copy()\n",
    "            strategy_data['signal'] = signals\n",
    "            strategy_data['signal_confidence'] = confidence\n",
    "            \n",
    "            # Run individual backtest\n",
    "            backtester = FluidTradeBacktester(initial_capital=self.initial_capital)\n",
    "            performance = backtester.run_backtest(strategy_data, strategy)\n",
    "            \n",
    "            strategy_results[name] = {\n",
    "                'data': strategy_data,\n",
    "                'performance': performance,\n",
    "                'backtester': backtester\n",
    "            }\n",
    "        \n",
    "        # Combine strategies with dynamic allocation\n",
    "        portfolio_equity = []\n",
    "        portfolio_positions = {}\n",
    "        \n",
    "        for i in range(len(data)):\n",
    "            row = data.iloc[i]\n",
    "            timestamp = row['timestamp']\n",
    "            \n",
    "            # Calculate current market state\n",
    "            market_state = {\n",
    "                'turbulence': row.get('turbulence', 0.3),\n",
    "                'volatility': row.get('realized_vol', 0.02),\n",
    "                'energy': row.get('kinetic_energy', 0.1)\n",
    "            }\n",
    "            \n",
    "            # Get dynamic allocations\n",
    "            current_allocations = self.dynamic_allocation(market_state)\n",
    "            \n",
    "            # Calculate portfolio value (simplified)\n",
    "            portfolio_value = 0\n",
    "            \n",
    "            for strategy_name, allocation in current_allocations.items():\n",
    "                strategy_result = strategy_results[strategy_name]\n",
    "                \n",
    "                # Get strategy equity at this point\n",
    "                if i < len(strategy_result['performance']['equity_curve']):\n",
    "                    strategy_equity = strategy_result['performance']['equity_curve']['equity'].iloc[i]\n",
    "                    portfolio_value += strategy_equity * allocation\n",
    "            \n",
    "            portfolio_equity.append({\n",
    "                'timestamp': timestamp,\n",
    "                'equity': portfolio_value,\n",
    "                'allocations': current_allocations.copy()\n",
    "            })\n",
    "        \n",
    "        # Calculate portfolio performance metrics\n",
    "        portfolio_df = pd.DataFrame(portfolio_equity)\n",
    "        portfolio_returns = portfolio_df['equity'].pct_change().dropna()\n",
    "        \n",
    "        portfolio_performance = {\n",
    "            'total_return': (portfolio_df['equity'].iloc[-1] - self.initial_capital) / self.initial_capital,\n",
    "            'sharpe_ratio': portfolio_returns.mean() / portfolio_returns.std() * np.sqrt(252 * 1000) if portfolio_returns.std() > 0 else 0,\n",
    "            'max_drawdown': self.calculate_max_drawdown(portfolio_df['equity']),\n",
    "            'var_95': self.calculate_portfolio_var(portfolio_returns, 0.05)[0] if len(portfolio_returns) > 30 else 0,\n",
    "            'equity_curve': portfolio_df,\n",
    "            'strategy_results': strategy_results\n",
    "        }\n",
    "        \n",
    "        print(f\"\\n Portfolio backtest complete!\")\n",
    "        print(f\"=Ê Portfolio Return: {portfolio_performance['total_return']*100:.2f}%\")\n",
    "        print(f\"=È Portfolio Sharpe: {portfolio_performance['sharpe_ratio']:.3f}\")\n",
    "        print(f\"=É Portfolio Max DD: {portfolio_performance['max_drawdown']*100:.2f}%\")\n",
    "        \n",
    "        return portfolio_performance\n",
    "    \n",
    "    def calculate_max_drawdown(self, equity_series):\n",
    "        \"\"\"\n",
    "        Calculate maximum drawdown.\n",
    "        \"\"\"\n",
    "        peak = equity_series.expanding().max()\n",
    "        drawdown = (equity_series - peak) / peak\n",
    "        return abs(drawdown.min())\n",
    "\n",
    "def create_risk_report(portfolio_performance):\n",
    "    \"\"\"\n",
    "    Create comprehensive risk analysis report.\n",
    "    \"\"\"\n",
    "    print(\"\\n=á FluidTrade Portfolio Risk Analysis Report\")\n",
    "    print(\"P\" * 50)\n",
    "    \n",
    "    # Portfolio level metrics\n",
    "    print(f\"\\n=Ê Portfolio Performance:\")\n",
    "    print(f\"Total Return: {portfolio_performance['total_return']*100:.2f}%\")\n",
    "    print(f\"Sharpe Ratio: {portfolio_performance['sharpe_ratio']:.3f}\")\n",
    "    print(f\"Maximum Drawdown: {portfolio_performance['max_drawdown']*100:.2f}%\")\n",
    "    print(f\"Value at Risk (95%): {portfolio_performance['var_95']*100:.2f}%\")\n",
    "    \n",
    "    # Individual strategy analysis\n",
    "    print(f\"\\n=È Individual Strategy Performance:\")\n",
    "    strategy_results = portfolio_performance['strategy_results']\n",
    "    \n",
    "    for name, result in strategy_results.items():\n",
    "        perf = result['performance']\n",
    "        print(f\"\\n  <¯ {name}:\")\n",
    "        print(f\"    Return: {perf['total_return']*100:.2f}%\")\n",
    "        print(f\"    Sharpe: {perf['sharpe_ratio']:.3f}\")\n",
