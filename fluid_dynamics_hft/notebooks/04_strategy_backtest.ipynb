    "\n",
    "def create_ensemble_strategy(data, top_strategies, ensemble_method='weighted_avg'):\n",
    "    \"\"\"\n",
    "    Create ensemble strategy from multiple parameter sets.\n",
    "    \"\"\"\n",
    "    print(f\"> Creating ensemble strategy with {len(top_strategies)} components...\")\n",
    "    \n",
    "    ensemble_signals = pd.Series(0.0, index=data.index)\n",
    "    ensemble_confidence = pd.Series(0.0, index=data.index)\n",
    "    \n",
    "    strategy_weights = []\n",
    "    \n",
    "    for i, strategy_result in top_strategies.iterrows():\n",
    "        params = strategy_result['params']\n",
    "        \n",
    "        # Create strategy\n",
    "        strategy = FluidTradeStrategy(params)\n",
    "        \n",
    "        # Generate signals\n",
    "        signals, confidence = strategy.generate_combined_signals(data)\n",
    "        \n",
    "        # Calculate weight based on performance metric\n",
    "        if ensemble_method == 'weighted_avg':\n",
    "            weight = strategy_result['sharpe_ratio'] / top_strategies['sharpe_ratio'].sum()\n",
    "        else:  # equal weight\n",
    "            weight = 1.0 / len(top_strategies)\n",
    "        \n",
    "        strategy_weights.append(weight)\n",
    "        \n",
    "        # Combine signals\n",
    "        ensemble_signals += signals * confidence * weight\n",
    "        ensemble_confidence += confidence * weight\n",
    "    \n",
    "    # Normalize and threshold the ensemble signals\n",
    "    final_signals = pd.Series(0, index=data.index)\n",
    "    final_confidence = ensemble_confidence\n",
    "    \n",
    "    # Apply threshold for signal generation\n",
    "    signal_threshold = 0.3\n",
    "    confidence_threshold = 0.4\n",
    "    \n",
    "    buy_mask = (ensemble_signals > signal_threshold) & (ensemble_confidence > confidence_threshold)\n",
    "    sell_mask = (ensemble_signals < -signal_threshold) & (ensemble_confidence > confidence_threshold)\n",
    "    \n",
    "    final_signals[buy_mask] = 1\n",
    "    final_signals[sell_mask] = -1\n",
    "    \n",
    "    print(f\" Ensemble strategy created!\")\n",
    "    print(f\"=Ê Strategy weights: {[f'{w:.3f}' for w in strategy_weights]}\")\n",
    "    print(f\"=È Ensemble signals: {(final_signals != 0).sum()}\")\n",
    "    \n",
    "    return final_signals, final_confidence\n",
    "\n",
    "# Define parameter ranges for optimization\n",
