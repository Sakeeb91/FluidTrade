    "class FluidTradeBacktester:\n",
    "    \"\"\"\n",
    "    Professional backtesting engine with realistic trading conditions.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, initial_capital=100000, max_positions=5):\n",
    "        self.initial_capital = initial_capital\n",
    "        self.max_positions = max_positions\n",
    "        \n",
    "        # Trading state\n",
    "        self.cash = initial_capital\n",
    "        self.positions = {}  # {entry_time: {size, entry_price, direction, stop_loss, take_profit}}\n",
    "        self.closed_trades = []\n",
    "        self.equity_curve = []\n",
    "        self.drawdowns = []\n",
    "        \n",
    "        # Performance tracking\n",
    "        self.total_trades = 0\n",
    "        self.winning_trades = 0\n",
    "        self.total_pnl = 0\n",
    "        self.max_drawdown = 0\n",
    "        self.peak_equity = initial_capital\n",
    "        \n",
    "    def calculate_transaction_cost(self, price, size, transaction_cost_rate):\n",
    "        \"\"\"\n",
    "        Calculate realistic transaction costs.\n",
    "        \"\"\"\n",
    "        # Spread cost\n",
    "        spread_cost = price * size * transaction_cost_rate\n",
    "        \n",
    "        # Market impact (simplified)\n",
    "        impact_cost = price * size * 0.0001 * np.sqrt(size / 0.01)  # Increases with size\n",
    "        \n",
    "        return spread_cost + impact_cost\n",
    "    \n",
    "    def open_position(self, timestamp, price, direction, size, confidence, stop_loss_pct, take_profit_pct, transaction_cost):\n",
    "        \"\"\"\n",
    "        Open a new position with risk management.\n",
    "        \"\"\"\n",
    "        if len(self.positions) >= self.max_positions:\n",
    "            return False\n",
    "        \n",
    "        # Calculate costs\n",
    "        position_value = price * size\n",
    "        costs = self.calculate_transaction_cost(price, size, transaction_cost)\n",
    "        \n",
    "        if position_value + costs > self.cash:\n",
    "            return False\n",
    "        \n",
    "        # Set stop loss and take profit\n",
    "        if direction == 1:  # Long\n",
    "            stop_loss = price * (1 - stop_loss_pct)\n",
    "            take_profit = price * (1 + take_profit_pct)\n",
    "        else:  # Short\n",
    "            stop_loss = price * (1 + stop_loss_pct)\n",
    "            take_profit = price * (1 - take_profit_pct)\n",
    "        \n",
    "        # Create position\n",
    "        position = {\n",
    "            'direction': direction,\n",
    "            'size': size,\n",
    "            'entry_price': price,\n",
    "            'entry_time': timestamp,\n",
    "            'stop_loss': stop_loss,\n",
    "            'take_profit': take_profit,\n",
    "            'confidence': confidence,\n",
    "            'entry_costs': costs\n",
    "        }\n",
    "        \n",
    "        self.positions[timestamp] = position\n",
    "        self.cash -= (position_value + costs)\n",
    "        \n",
    "        return True\n",
    "    \n",
    "    def close_position(self, position_key, timestamp, price, reason, transaction_cost):\n",
    "        \"\"\"\n",
    "        Close an existing position.\n",
    "        \"\"\"\n",
    "        position = self.positions[position_key]\n",
    "        \n",
    "        # Calculate PnL\n",
    "        if position['direction'] == 1:  # Long\n",
    "            pnl = (price - position['entry_price']) * position['size']\n",
    "        else:  # Short\n",
    "            pnl = (position['entry_price'] - price) * position['size']\n",
    "        \n",
    "        # Calculate exit costs\n",
    "        exit_costs = self.calculate_transaction_cost(price, position['size'], transaction_cost)\n",
    "        \n",
    "        # Net PnL after costs\n",
    "        net_pnl = pnl - position['entry_costs'] - exit_costs\n",
    "        \n",
    "        # Update cash\n",
    "        position_value = price * position['size']\n",
    "        self.cash += position_value + net_pnl\n",
    "        \n",
    "        # Record trade\n",
    "        trade = {\n",
    "            'entry_time': position['entry_time'],\n",
    "            'exit_time': timestamp,\n",
    "            'direction': position['direction'],\n",
    "            'size': position['size'],\n",
    "            'entry_price': position['entry_price'],\n",
    "            'exit_price': price,\n",
    "            'pnl': net_pnl,\n",
    "            'return': net_pnl / (position['entry_price'] * position['size']),\n",
    "            'confidence': position['confidence'],\n",
    "            'exit_reason': reason,\n",
    "            'holding_time': timestamp - position['entry_time']\n",
    "        }\n",
    "        \n",
    "        self.closed_trades.append(trade)\n",
    "        self.total_trades += 1\n",
    "        if net_pnl > 0:\n",
    "            self.winning_trades += 1\n",
    "        self.total_pnl += net_pnl\n",
    "        \n",
    "        # Remove position\n",
    "        del self.positions[position_key]\n",
    "        \n",
    "        return trade\n",
    "    \n",
    "    def update_equity(self, timestamp, current_prices):\n",
    "        \"\"\"\n",
    "        Update equity curve and drawdown tracking.\n",
    "        \"\"\"\n",
    "        # Calculate mark-to-market value of open positions\n",
    "        mtm_value = 0\n",
    "        for pos_key, position in self.positions.items():\n",
    "            current_price = current_prices\n",
    "            if position['direction'] == 1:  # Long\n",
    "                unrealized_pnl = (current_price - position['entry_price']) * position['size']\n",
    "            else:  # Short\n",
    "                unrealized_pnl = (position['entry_price'] - current_price) * position['size']\n",
    "            \n",
    "            mtm_value += position['entry_price'] * position['size'] + unrealized_pnl\n",
    "        \n",
    "        # Total equity\n",
    "        total_equity = self.cash + mtm_value\n",
    "        \n",
    "        # Update peak and drawdown\n",
    "        if total_equity > self.peak_equity:\n",
    "            self.peak_equity = total_equity\n",
    "        \n",
    "        current_drawdown = (self.peak_equity - total_equity) / self.peak_equity\n",
    "        if current_drawdown > self.max_drawdown:\n",
    "            self.max_drawdown = current_drawdown\n",
    "        \n",
    "        self.equity_curve.append({\n",
    "            'timestamp': timestamp,\n",
    "            'equity': total_equity,\n",
    "            'cash': self.cash,\n",
    "            'positions_value': mtm_value,\n",
    "            'drawdown': current_drawdown\n",
    "        })\n",
    "    \n",
    "    def run_backtest(self, data, strategy):\n",
    "        \"\"\"\n",
    "        Run complete backtest with the given strategy.\n",
    "        \"\"\"\n",
    "        print(\"<Æ Starting comprehensive backtest...\")\n",
    "        \n",
    "        for i in range(len(data)):\n",
    "            row = data.iloc[i]\n",
    "            timestamp = row['timestamp']\n",
    "            price = row['price']\n",
    "            signal = row['signal']\n",
    "            confidence = row['signal_confidence']\n",
    "            transaction_cost = row['transaction_cost']\n",
    "            \n",
    "            # Check for position exits (stop loss, take profit, time-based)\n",
    "            positions_to_close = []\n",
    "            for pos_key, position in self.positions.items():\n",
    "                # Stop loss check\n",
    "                if ((position['direction'] == 1 and price <= position['stop_loss']) or\n",
    "                    (position['direction'] == -1 and price >= position['stop_loss'])):\n",
    "                    positions_to_close.append((pos_key, 'stop_loss'))\n",
    "                \n",
    "                # Take profit check\n",
    "                elif ((position['direction'] == 1 and price >= position['take_profit']) or\n",
    "                      (position['direction'] == -1 and price <= position['take_profit'])):\n",
    "                    positions_to_close.append((pos_key, 'take_profit'))\n",
    "                \n",
    "                # Time-based exit (hold for max time)\n",
    "                elif timestamp - position['entry_time'] > strategy.params['position_hold_time']:\n",
    "                    positions_to_close.append((pos_key, 'time_exit'))\n",
    "            \n",
    "            # Close positions\n",
    "            for pos_key, reason in positions_to_close:\n",
    "                self.close_position(pos_key, timestamp, price, reason, transaction_cost)\n",
    "            \n",
    "            # Check for new position entries\n",
    "            if signal != 0 and confidence > 0.5:  # Only trade high-confidence signals\n",
    "                # Calculate position size\n",
    "                position_size = strategy.calculate_position_size(row, confidence, price)\n",
    "                \n",
    "                # Open position\n",
    "                success = self.open_position(\n",
    "                    timestamp=timestamp,\n",
    "                    price=price,\n",
    "                    direction=signal,\n",
    "                    size=position_size,\n",
    "                    confidence=confidence,\n",
    "                    stop_loss_pct=strategy.params['stop_loss'],\n",
    "                    take_profit_pct=strategy.params['take_profit'],\n",
    "                    transaction_cost=transaction_cost\n",
    "                )\n",
    "            \n",
    "            # Update equity tracking\n",
    "            self.update_equity(timestamp, price)\n",
    "        \n",
    "        # Close any remaining positions at the end\n",
    "        final_price = data['price'].iloc[-1]\n",
    "        final_timestamp = data['timestamp'].iloc[-1]\n",
    "        final_transaction_cost = data['transaction_cost'].iloc[-1]\n",
    "        \n",
    "        remaining_positions = list(self.positions.keys())\n",
    "        for pos_key in remaining_positions:\n",
    "            self.close_position(pos_key, final_timestamp, final_price, 'final_exit', final_transaction_cost)\n",
    "        \n",
    "        print(f\" Backtest complete!\")\n",
    "        print(f\"=Ê Total trades: {self.total_trades}\")\n",
    "        print(f\"<¯ Win rate: {self.winning_trades/max(1, self.total_trades)*100:.1f}%\")\n",
    "        print(f\"=° Total PnL: ${self.total_pnl:.2f}\")\n",
    "        print(f\"=É Max drawdown: {self.max_drawdown*100:.2f}%\")\n",
    "        \n",
    "        return self.calculate_performance_metrics()\n",
    "    \n",
    "    def calculate_performance_metrics(self):\n",
    "        \"\"\"\n",
    "        Calculate comprehensive performance metrics.\n",
    "        \"\"\"\n",
    "        if not self.closed_trades:\n",
    "            return {}\n",
    "        \n",
    "        # Convert to DataFrame for easier analysis\n",
    "        trades_df = pd.DataFrame(self.closed_trades)\n",
    "        equity_df = pd.DataFrame(self.equity_curve)\n",
    "        \n",
    "        # Basic metrics\n",
    "        total_return = (equity_df['equity'].iloc[-1] - self.initial_capital) / self.initial_capital\n",
    "        \n",
    "        # Calculate returns for Sharpe ratio\n",
    "        equity_returns = equity_df['equity'].pct_change().dropna()\n",
    "        \n",
    "        # Risk metrics\n",
    "        sharpe_ratio = equity_returns.mean() / equity_returns.std() * np.sqrt(252 * 1000) if equity_returns.std() > 0 else 0\n",
    "        sortino_ratio = equity_returns.mean() / equity_returns[equity_returns < 0].std() * np.sqrt(252 * 1000) if len(equity_returns[equity_returns < 0]) > 0 else 0\n",
    "        \n",
    "        # Trade analysis\n",
    "        avg_win = trades_df[trades_df['pnl'] > 0]['pnl'].mean() if len(trades_df[trades_df['pnl'] > 0]) > 0 else 0\n",
    "        avg_loss = trades_df[trades_df['pnl'] < 0]['pnl'].mean() if len(trades_df[trades_df['pnl'] < 0]) > 0 else 0\n",
    "        profit_factor = -avg_win * len(trades_df[trades_df['pnl'] > 0]) / (avg_loss * len(trades_df[trades_df['pnl'] < 0])) if avg_loss != 0 else float('inf')\n",
    "        \n",
    "        metrics = {\n",
    "            'total_return': total_return,\n",
    "            'annualized_return': total_return * 252 * 1000 / len(equity_df),  # Approximate annualization\n",
    "            'sharpe_ratio': sharpe_ratio,\n",
    "            'sortino_ratio': sortino_ratio,\n",
    "            'max_drawdown': self.max_drawdown,\n",
    "            'total_trades': self.total_trades,\n",
    "            'win_rate': self.winning_trades / max(1, self.total_trades),\n",
    "            'profit_factor': profit_factor,\n",
    "            'avg_win': avg_win,\n",
    "            'avg_loss': avg_loss,\n",
    "            'avg_holding_time': trades_df['holding_time'].mean(),\n",
    "            'final_equity': equity_df['equity'].iloc[-1],\n",
    "            'equity_curve': equity_df,\n",
    "            'trades': trades_df\n",
    "        }\n",
    "        \n",
    "        return metrics\n",
    "\n",
