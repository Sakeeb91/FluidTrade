    "    \n",
    "    print(f\"\\n<¯ Recommended Portfolio Allocation (based on risk-adjusted returns):\")\n",
    "    total_score = sum(score for _, score, _ in strategies_ranked)\n",
    "    for name, score, _ in strategies_ranked:\n",
    "        allocation = score / total_score if total_score > 0 else 1 / len(strategies_ranked)\n",
    "        print(f\"  " {name}: {allocation*100:.1f}%\")\n",
    "\n",
    "# Create multi-strategy portfolio\n",
    "print(\"<× Creating FluidTrade portfolio with multiple strategies...\")\n",
    "\n",
    "portfolio_manager = FluidTradePortfolioManager(initial_capital=100000, risk_budget=0.02)\n",
