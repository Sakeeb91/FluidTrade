    "def generate_comprehensive_trading_data(n_points=50000, dt=0.001):\n",
    "    \"\"\"\n",
    "    Generate comprehensive market data for strategy testing.\n",
    "    Includes realistic market conditions, transaction costs, and market impact.\n",
    "    \"\"\"\n",
    "    times = np.arange(n_points) * dt\n",
    "    \n",
    "    # Define market regimes with realistic transitions\n",
    "    regimes = {\n",
    "        'bull_trend': (0, 8000, {'drift': 0.08, 'vol': 0.015, 'mean_revert': -0.05}),\n",
    "        'volatile': (7000, 15000, {'drift': 0.02, 'vol': 0.035, 'mean_revert': -0.1}),\n",
    "        'bear_trend': (14000, 22000, {'drift': -0.06, 'vol': 0.02, 'mean_revert': -0.08}),\n",
    "        'sideways': (21000, 29000, {'drift': 0.005, 'vol': 0.01, 'mean_revert': -0.15}),\n",
    "        'recovery': (28000, 36000, {'drift': 0.12, 'vol': 0.025, 'mean_revert': -0.03}),\n",
    "        'crash': (35000, 37000, {'drift': -0.30, 'vol': 0.08, 'mean_revert': 0.0}),\n",
    "        'post_crash': (36500, 44000, {'drift': 0.15, 'vol': 0.04, 'mean_revert': -0.02}),\n",
    "        'normal': (43000, 50000, {'drift': 0.03, 'vol': 0.018, 'mean_revert': -0.08})\n",
    "    }\n",
    "    \n",
    "    # Initialize arrays\n",
    "    prices = np.zeros(n_points)\n",
    "    volumes = np.zeros(n_points)\n",
    "    order_flows = np.zeros(n_points)\n",
    "    transaction_costs = np.zeros(n_points)\n",
    "    regime_labels = ['normal'] * n_points\n",
    "    \n",
    "    base_price = 100.0\n",
    "    current_price = base_price\n",
    "    price_history = [base_price]\n",
    "    \n",
    "    for i, time in enumerate(times):\n",
    "        # Determine active regime\n",
    "        active_regime = 'normal'\n",
    "        regime_params = {'drift': 0.03, 'vol': 0.018, 'mean_revert': -0.08}\n",
    "        \n",
    "        for regime_name, (start, end, params) in regimes.items():\n",
    "            if start <= i < end:\n",
    "                active_regime = regime_name\n",
    "                regime_params = params\n",
    "                break\n",
    "        \n",
    "        regime_labels[i] = active_regime\n",
    "        \n",
    "        # Generate price movement\n",
    "        drift = regime_params['drift']\n",
    "        volatility = regime_params['vol']\n",
    "        mean_reversion = regime_params['mean_revert']\n",
    "        \n",
    "        # Mean reversion component\n",
    "        if len(price_history) > 100:\n",
    "            ma_100 = np.mean(price_history[-100:])\n",
    "            mr_component = mean_reversion * (current_price - ma_100) / ma_100\n",
    "        else:\n",
    "            mr_component = 0\n",
    "        \n",
    "        # Add microstructure noise and jump components\n",
    "        jump_prob = 0.001  # 0.1% chance of jump per period\n",
    "        jump_component = 0\n",
    "        if np.random.random() < jump_prob:\n",
    "            jump_component = np.random.normal(0, 0.02)  # 2% jump size\n",
    "        \n",
    "        # Combine all components\n",
    "        total_return = (drift + mr_component) * dt + volatility * np.sqrt(dt) * np.random.randn() + jump_component\n",
    "        \n",
    "        # Apply price change\n",
    "        current_price *= (1 + total_return)\n",
    "        prices[i] = current_price\n",
    "        price_history.append(current_price)\n",
    "        \n",
    "        # Generate volume (higher during volatile periods)\n",
    "        base_volume = 1000\n",
    "        vol_multiplier = 1 + 3 * volatility + 2 * abs(total_return)\n",
    "        volumes[i] = base_volume * vol_multiplier * (1 + 0.3 * np.random.exponential(1))\n",
    "        \n",
    "        # Generate order flow with momentum\n",
    "        momentum = np.sign(total_return) * min(abs(total_return) * 10000, 1000)\n",
    "        order_flows[i] = momentum + 200 * np.random.randn()\n",
    "        \n",
    "        # Transaction costs (bid-ask spread + market impact)\n",
    "        base_spread = 0.0001  # 1 bps base spread\n",
    "        vol_spread = volatility * 0.01  # Spread widens with volatility\n",
    "        transaction_costs[i] = base_spread + vol_spread\n",
    "    \n",
    "    # Calculate additional market microstructure variables\n",
    "    spreads = transaction_costs\n",
    "    bids = prices - spreads * prices / 2\n",
    "    asks = prices + spreads * prices / 2\n",
    "    \n",
    "    # Create comprehensive DataFrame\n",
    "    data = pd.DataFrame({\n",
    "        'timestamp': times,\n",
    "        'price': prices,\n",
    "        'bid': bids,\n",
    "        'ask': asks,\n",
    "        'volume': volumes,\n",
    "        'order_flow': order_flows,\n",
    "        'spread': spreads,\n",
    "        'transaction_cost': transaction_costs,\n",
    "        'regime_label': regime_labels\n",
    "    })\n",
    "    \n",
    "    return data\n",
    "\n",
    "def comprehensive_fluid_transform(data):\n",
    "    \"\"\"\n",
    "    Complete fluid dynamics transformation with all features needed for trading.\n",
    "    \"\"\"\n",
    "    fluid_data = data.copy()\n",
    "    \n",
    "    # Basic fluid variables\n",
    "    volume_density = data['volume'] / data['volume'].max()\n",
    "    fluid_data['u_velocity'] = data['order_flow'] / (volume_density + 0.01)\n",
    "    \n",
    "    price_returns = data['price'].pct_change().fillna(0)\n",
    "    fluid_data['v_velocity'] = gaussian_filter1d(price_returns * 1000, sigma=2)\n",
    "    \n",
    "    mean_spread = data['spread'].mean()\n",
    "    fluid_data['pressure'] = -np.log(data['spread'] / mean_spread)\n",
    "    fluid_data['density'] = volume_density\n",
    "    \n",
    "    # Smooth velocity fields\n",
    "    u_smooth = gaussian_filter1d(fluid_data['u_velocity'], sigma=3)\n",
    "    v_smooth = gaussian_filter1d(fluid_data['v_velocity'], sigma=3)\n",
    "    \n",
    "    # Core fluid properties\n",
    "    fluid_data['vorticity'] = np.gradient(v_smooth) - np.gradient(u_smooth)\n",
    "    fluid_data['velocity_magnitude'] = np.sqrt(u_smooth**2 + v_smooth**2)\n",
    "    fluid_data['divergence'] = np.gradient(u_smooth) + np.gradient(v_smooth)\n",
    "    fluid_data['kinetic_energy'] = 0.5 * fluid_data['density'] * fluid_data['velocity_magnitude']**2\n",
    "    \n",
    "    # Advanced features for trading\n",
    "    fluid_data['pressure_gradient'] = np.gradient(fluid_data['pressure'])\n",
    "    fluid_data['enstrophy'] = 0.5 * fluid_data['vorticity']**2\n",
    "    fluid_data['strain_rate'] = np.abs(np.gradient(u_smooth)) + np.abs(np.gradient(v_smooth))\n",
    "    fluid_data['q_criterion'] = 0.5 * (fluid_data['vorticity']**2 - fluid_data['strain_rate']**2)\n",
    "    \n",
    "    # Multi-scale vorticity for different timeframes\n",
    "    fluid_data['vorticity_fast'] = np.gradient(gaussian_filter1d(v_smooth, sigma=1)) - np.gradient(gaussian_filter1d(u_smooth, sigma=1))\n",
    "    fluid_data['vorticity_slow'] = np.gradient(gaussian_filter1d(v_smooth, sigma=8)) - np.gradient(gaussian_filter1d(u_smooth, sigma=8))\n",
    "    \n",
    "    # Momentum and trend indicators\n",
    "    fluid_data['momentum'] = fluid_data['velocity_magnitude'].rolling(50).mean()\n",
    "    fluid_data['energy_trend'] = fluid_data['kinetic_energy'].rolling(100).apply(\n",
    "        lambda x: np.polyfit(range(len(x)), x, 1)[0] if len(x) == 100 else 0\n",
    "    )\n",
    "    \n",
    "    # Volatility measures\n",
    "    fluid_data['realized_vol'] = price_returns.rolling(100).std() * np.sqrt(1000)  # Annualized\n",
    "    fluid_data['fluid_vol'] = fluid_data['velocity_magnitude'].rolling(100).std()\n",
    "    \n",
    "    # Regime indicators\n",
    "    fluid_data['turbulence'] = (\n",
    "        fluid_data['vorticity'].rolling(50).std() * 0.4 +\n",
    "        fluid_data['velocity_magnitude'].rolling(50).std() * 0.3 +\n",
    "        fluid_data['divergence'].rolling(50).std() * 0.3\n",
    "    )\n",
    "    \n",
    "    return fluid_data\n",
    "\n",
    "# Generate comprehensive trading dataset\n",
    "print(\"= Generating comprehensive trading dataset...\")\n",
    "trading_data = generate_comprehensive_trading_data(n_points=50000)\n",
    "fluid_trading_data = comprehensive_fluid_transform(trading_data)\n",
    "\n",
    "print(f\" Generated {len(fluid_trading_data)} data points for strategy testing\")\n",
    "print(f\"=Ê Time range: {fluid_trading_data['timestamp'].min():.1f}s to {fluid_trading_data['timestamp'].max():.1f}s\")\n",
    "print(f\"=° Price range: ${fluid_trading_data['price'].min():.2f} to ${fluid_trading_data['price'].max():.2f}\")\n",
    "\n",
    "# Show regime distribution\n",
    "regime_counts = fluid_trading_data['regime_label'].value_counts()\n",
    "print(\"\\n=È Market Regime Distribution:\")\n",
    "for regime, count in regime_counts.items():\n",
    "    print(f\"  " {regime}: {count} points ({count/len(fluid_trading_data)*100:.1f}%)\")\n",
    "\n",
    "# Display sample data\n",
    "print(\"\\n=Ê Sample Trading Data:\")\n",
    "sample_cols = ['timestamp', 'price', 'vorticity', 'kinetic_energy', 'turbulence', 'regime_label']\n",
    "fluid_trading_data[sample_cols].head(10)"
   ]
