    "    \n",
    "    # Risk-adjusted returns comparison\n",
    "    print(f\"\\n=Ê Risk-Adjusted Performance Ranking:\")\n",
    "    strategies_ranked = []\n",
    "    \n",
    "    for name, result in strategy_results.items():\n",
    "        perf = result['performance']\n",
