    "class FluidTradeStrategy:\n",
    "    \"\"\"\n",
    "    Complete trading strategy based on fluid dynamics patterns.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, strategy_params=None):\n",
    "        # Default strategy parameters\n",
    "        self.params = strategy_params or {\n",
    "            'vortex_threshold': 1.5,\n",
    "            'vortex_confidence': 0.7,\n",
    "            'energy_percentile': 80,\n",
    "            'turbulence_threshold': 0.3,\n",
    "            'momentum_window': 50,\n",
    "            'position_hold_time': 100,\n",
    "            'stop_loss': 0.02,\n",
    "            'take_profit': 0.04,\n",
    "            'max_position_size': 0.1,\n",
    "            'risk_factor': 0.02\n",
    "        }\n",
    "        \n",
    "        self.positions = []\n",
    "        self.signals = []\n",
    "        self.performance_metrics = {}\n",
    "    \n",
    "    def detect_vortex_signals(self, data):\n",
    "        \"\"\"\n",
    "        Detect trading signals from vortex patterns.\n",
    "        \"\"\"\n",
    "        signals = pd.Series(0, index=data.index)  # 0 = no signal, 1 = buy, -1 = sell\n",
    "        confidence = pd.Series(0.0, index=data.index)\n",
    "        \n",
    "        # Calculate Q-criterion for vortex detection\n",
    "        q_criterion = data['q_criterion']\n",
    "        vorticity = data['vorticity']\n",
    "        \n",
    "        # Multi-timeframe vorticity agreement\n",
    "        vorticity_fast = data['vorticity_fast']\n",
    "        vorticity_slow = data['vorticity_slow']\n",
    "        \n",
    "        for i in range(len(data)):\n",
    "            if i < self.params['momentum_window']:\n",
    "                continue\n",
    "            \n",
    "            # Check vortex conditions\n",
    "            strong_vortex = abs(vorticity.iloc[i]) > self.params['vortex_threshold']\n",
    "            positive_q = q_criterion.iloc[i] > 0.2\n",
    "            \n",
    "            # Multi-timeframe agreement\n",
    "            fast_slow_agreement = np.sign(vorticity_fast.iloc[i]) == np.sign(vorticity_slow.iloc[i])\n",
    "            \n",
    "            if strong_vortex and positive_q and fast_slow_agreement:\n",
    "                # Signal direction based on vorticity\n",
    "                # Counterclockwise (negative) vortex -> bullish reversal -> buy\n",
    "                # Clockwise (positive) vortex -> bearish reversal -> sell\n",
    "                signal_direction = -np.sign(vorticity.iloc[i])  # Contrarian to vortex direction\n",
    "                signals.iloc[i] = signal_direction\n",
    "                \n",
    "                # Calculate confidence based on multiple factors\n",
    "                vortex_strength = abs(vorticity.iloc[i]) / self.params['vortex_threshold']\n",
    "                q_strength = max(0, q_criterion.iloc[i]) / 0.5\n",
    "                agreement_bonus = 0.3 if fast_slow_agreement else 0\n",
    "                \n",
    "                confidence.iloc[i] = min(1.0, 0.4 * vortex_strength + 0.3 * q_strength + agreement_bonus)\n",
    "        \n",
    "        return signals, confidence\n",
    "    \n",
    "    def detect_regime_signals(self, data):\n",
    "        \"\"\"\n",
    "        Detect signals from regime changes and momentum.\n",
    "        \"\"\"\n",
    "        signals = pd.Series(0, index=data.index)\n",
    "        confidence = pd.Series(0.0, index=data.index)\n",
    "        \n",
    "        turbulence = data['turbulence']\n",
    "        momentum = data['momentum']\n",
    "        energy_trend = data['energy_trend']\n",
    "        \n",
    "        for i in range(len(data)):\n",
    "            if i < self.params['momentum_window']:\n",
    "                continue\n",
    "            \n",
    "            # Low turbulence = trending regime\n",
    "            low_turbulence = turbulence.iloc[i] < self.params['turbulence_threshold']\n",
    "            \n",
    "            # Positive momentum and energy trend\n",
    "            positive_momentum = momentum.iloc[i] > momentum.iloc[i-10:i].mean()\n",
    "            positive_energy_trend = energy_trend.iloc[i] > 0\n",
    "            \n",
    "            if low_turbulence and positive_momentum and positive_energy_trend:\n",
    "                signals.iloc[i] = 1  # Buy signal for uptrend\n",
    "                confidence.iloc[i] = 0.6\n",
    "            elif low_turbulence and not positive_momentum and energy_trend.iloc[i] < 0:\n",
    "                signals.iloc[i] = -1  # Sell signal for downtrend\n",
    "                confidence.iloc[i] = 0.6\n",
    "        \n",
    "        return signals, confidence\n",
    "    \n",
    "    def detect_energy_signals(self, data):\n",
    "        \"\"\"\n",
    "        Detect signals from energy spikes and patterns.\n",
    "        \"\"\"\n",
    "        signals = pd.Series(0, index=data.index)\n",
    "        confidence = pd.Series(0.0, index=data.index)\n",
    "        \n",
    "        kinetic_energy = data['kinetic_energy']\n",
    "        energy_threshold = kinetic_energy.quantile(self.params['energy_percentile'] / 100)\n",
    "        \n",
    "        for i in range(50, len(data)):\n",
    "            # Energy spike detection\n",
    "            current_energy = kinetic_energy.iloc[i]\n",
    "            recent_avg_energy = kinetic_energy.iloc[i-50:i].mean()\n",
    "            \n",
    "            energy_spike = current_energy > energy_threshold and current_energy > 2 * recent_avg_energy\n",
    "            \n",
    "            if energy_spike:\n",
    "                # Energy spikes often precede volatility - reduce position or stay neutral\n",
    "                signals.iloc[i] = 0  # Neutral/exit signal\n",
    "                confidence.iloc[i] = 0.4\n",
    "        \n",
    "        return signals, confidence\n",
    "    \n",
    "    def calculate_position_size(self, data, signal_confidence, current_price):\n",
    "        \"\"\"\n",
    "        Calculate position size based on risk management and signal confidence.\n",
    "        \"\"\"\n",
    "        # Base position size\n",
    "        base_size = self.params['max_position_size']\n",
    "        \n",
    "        # Adjust for signal confidence\n",
    "        confidence_adj = signal_confidence\n",
    "        \n",
    "        # Adjust for market volatility (fluid volatility)\n",
    "        if 'fluid_vol' in data.columns and not pd.isna(data['fluid_vol']):\n",
    "            vol_adj = 1 / (1 + data['fluid_vol'] * 10)  # Reduce size in high volatility\n",
    "        else:\n",
    "            vol_adj = 1.0\n",
    "        \n",
    "        # Adjust for turbulence\n",
    "        if 'turbulence' in data.columns and not pd.isna(data['turbulence']):\n",
    "            turbulence_adj = 1 / (1 + data['turbulence'] * 5)\n",
    "        else:\n",
    "            turbulence_adj = 1.0\n",
    "        \n",
    "        final_size = base_size * confidence_adj * vol_adj * turbulence_adj\n",
    "        return max(0.01, min(self.params['max_position_size'], final_size))\n",
    "    \n",
    "    def generate_combined_signals(self, data):\n",
    "        \"\"\"\n",
    "        Combine all signal types into final trading signals.\n",
    "        \"\"\"\n",
    "        print(\"= Generating vortex signals...\")\n",
    "        vortex_signals, vortex_confidence = self.detect_vortex_signals(data)\n",
    "        \n",
    "        print(\"=Ê Generating regime signals...\")\n",
    "        regime_signals, regime_confidence = self.detect_regime_signals(data)\n",
    "        \n",
    "        print(\"¡ Generating energy signals...\")\n",
    "        energy_signals, energy_confidence = self.detect_energy_signals(data)\n",
    "        \n",
    "        # Combine signals with weighted approach\n",
    "        combined_signals = pd.Series(0, index=data.index)\n",
    "        combined_confidence = pd.Series(0.0, index=data.index)\n",
    "        \n",
    "        for i in range(len(data)):\n",
    "            # Weight the signals\n",
    "            vortex_weight = 0.5\n",
    "            regime_weight = 0.3\n",
    "            energy_weight = 0.2\n",
    "            \n",
    "            weighted_signal = (\n",
    "                vortex_signals.iloc[i] * vortex_weight * vortex_confidence.iloc[i] +\n",
    "                regime_signals.iloc[i] * regime_weight * regime_confidence.iloc[i] +\n",
    "                energy_signals.iloc[i] * energy_weight * energy_confidence.iloc[i]\n",
    "            )\n",
    "            \n",
    "            total_confidence = (\n",
    "                vortex_confidence.iloc[i] * vortex_weight +\n",
    "                regime_confidence.iloc[i] * regime_weight +\n",
    "                energy_confidence.iloc[i] * energy_weight\n",
    "            )\n",
    "            \n",
    "            # Apply threshold for signal generation\n",
    "            if abs(weighted_signal) > 0.3 and total_confidence > 0.4:\n",
    "                combined_signals.iloc[i] = np.sign(weighted_signal)\n",
    "                combined_confidence.iloc[i] = min(1.0, total_confidence)\n",
    "        \n",
    "        print(f\" Generated {(combined_signals != 0).sum()} combined signals\")\n",
    "        print(f\"=È Buy signals: {(combined_signals == 1).sum()}\")\n",
    "        print(f\"=É Sell signals: {(combined_signals == -1).sum()}\")\n",
    "        \n",
    "        return combined_signals, combined_confidence\n",
    "\n",
    "# Initialize strategy\n",
    "print(\"<¯ Initializing FluidTrade strategy...\")\n",
    "strategy = FluidTradeStrategy()\n",
    "\n",
    "# Generate signals\n",
    "print(\"= Generating trading signals...\")\n",
    "signals, signal_confidence = strategy.generate_combined_signals(fluid_trading_data)\n",
    "\n",
    "# Add signals to data\n",
    "fluid_trading_data['signal'] = signals\n",
    "fluid_trading_data['signal_confidence'] = signal_confidence\n",
    "\n",
    "print(\"\\n=Ê Signal Generation Summary:\")\n",
    "print(f\"Total signals: {(signals != 0).sum()}\")\n",
    "print(f\"Average confidence: {signal_confidence[signal_confidence > 0].mean():.3f}\")\n",
    "print(f\"Signal density: {(signals != 0).sum() / len(signals) * 100:.2f}%\")"
   ]
