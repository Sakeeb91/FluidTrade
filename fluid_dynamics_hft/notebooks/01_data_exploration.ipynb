    "def generate_synthetic_hft_data(n_points=10000, dt=0.001, base_price=100.0):\n",
    "    \"\"\"\n",
    "    Generate realistic high-frequency trading data with market microstructure.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    n_points : int\n",
    "        Number of time points\n",
    "    dt : float  \n",
    "        Time step in seconds (0.001 = 1ms)\n",
    "    base_price : float\n",
    "        Starting price level\n",
    "    \n",
    "    Returns:\n",
    "    --------\n",
    "    pd.DataFrame with columns: timestamp, price, bid, ask, volume, order_flow\n",
    "    \"\"\"\n",
    "    \n",
    "    # Time vector\n",
    "    times = np.arange(n_points) * dt\n",
    "    \n",
    "    # Generate price with trend + mean reversion + noise\n",
    "    trend = 0.02 * np.sin(2 * np.pi * times / 60)  # 1-minute cycle\n",
    "    mean_reversion = -0.1 * np.cumsum(np.random.randn(n_points)) * dt\n",
    "    microstructure_noise = 0.01 * np.random.randn(n_points)\n",
    "    \n",
    "    log_price = np.log(base_price) + np.cumsum(trend + mean_reversion) * dt + microstructure_noise\n",
    "    prices = np.exp(log_price)\n",
    "    \n",
    "    # Generate bid-ask spread (wider during volatile periods)\n",
    "    volatility = np.abs(np.diff(np.concatenate([[log_price[0]], log_price]))) * 100\n",
    "    spread = 0.01 + 0.02 * volatility  # Spread widens with volatility\n",
    "    \n",
    "    bids = prices - spread/2\n",
    "    asks = prices + spread/2\n",
    "    \n",
    "    # Generate volume (correlated with price movements)\n",
    "    price_changes = np.diff(np.concatenate([[prices[0]], prices]))\n",
    "    base_volume = 1000 + 500 * np.random.exponential(1, n_points)\n",
    "    volume = base_volume * (1 + 2 * np.abs(price_changes))\n",
    "    \n",
    "    # Generate order flow (buy/sell imbalance)\n",
    "    # Positive = more buying, Negative = more selling\n",
    "    order_flow = np.sign(price_changes) * volume + 100 * np.random.randn(n_points)\n",
    "    \n",
    "    # Create DataFrame\n",
    "    data = pd.DataFrame({\n",
    "        'timestamp': times,\n",
    "        'price': prices,\n",
    "        'bid': bids,\n",
    "        'ask': asks,\n",
    "        'volume': volume,\n",
    "        'order_flow': order_flow,\n",
    "        'spread': spread\n",
    "    })\n",
    "    \n",
    "    return data\n",
    "\n",
    "# Generate sample data\n",
    "print(\"= Generating synthetic HFT data...\")\n",
    "market_data = generate_synthetic_hft_data(n_points=10000)\n",
    "\n",
    "print(f\" Generated {len(market_data)} data points\")\n",
    "print(f\"=Ê Time range: {market_data['timestamp'].min():.3f}s to {market_data['timestamp'].max():.3f}s\")\n",
    "print(f\"=° Price range: ${market_data['price'].min():.2f} to ${market_data['price'].max():.2f}\")\n",
    "\n",
    "# Display first few rows\n",
    "market_data.head()"
