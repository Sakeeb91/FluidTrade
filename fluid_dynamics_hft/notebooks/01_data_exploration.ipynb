    "def detect_market_regimes(fluid_data, window=100):\n",
    "    \"\"\"\n",
    "    Detect market regimes using fluid dynamics indicators.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    fluid_data : pd.DataFrame\n",
    "        Data with fluid dynamics variables\n",
    "    window : int\n",
    "        Rolling window for regime detection\n",
    "    \n",
    "    Returns:\n",
    "    --------\n",
    "    pd.DataFrame with regime classifications\n",
    "    \"\"\"\n",
    "    \n",
    "    # Calculate turbulence indicators\n",
    "    turbulence_score = (\n",
    "        fluid_data['vorticity'].rolling(window).std() * 0.4 +\n",
    "        fluid_data['velocity_magnitude'].rolling(window).std() * 0.3 +\n",
    "        fluid_data['divergence'].rolling(window).std() * 0.3\n",
    "    )\n",
    "    \n",
    "    # Normalize turbulence score\n",
    "    turbulence_normalized = (turbulence_score - turbulence_score.min()) / (turbulence_score.max() - turbulence_score.min())\n",
    "    \n",
    "    # Define regime thresholds\n",
    "    laminar_threshold = 0.3\n",
    "    turbulent_threshold = 0.7\n",
    "    \n",
    "    # Classify regimes\n",
    "    regimes = []\n",
    "    for score in turbulence_normalized:\n",
    "        if pd.isna(score):\n",
    "            regimes.append('Unknown')\n",
    "        elif score < laminar_threshold:\n",
    "            regimes.append('Laminar')\n",
    "        elif score > turbulent_threshold:\n",
    "            regimes.append('Turbulent')\n",
    "        else:\n",
    "            regimes.append('Transition')\n",
    "    \n",
    "    result_data = fluid_data.copy()\n",
    "    result_data['turbulence_score'] = turbulence_normalized\n",
    "    result_data['regime'] = regimes\n",
    "    \n",
    "    return result_data\n",
    "\n",
    "# Detect market regimes\n",
    "print(\"= Detecting market regimes using fluid dynamics...\")\n",
    "regime_data = detect_market_regimes(fluid_data)\n",
    "\n",
    "# Regime statistics\n",
    "regime_counts = regime_data['regime'].value_counts()\n",
    "print(\"\\n=Ê Market Regime Distribution:\")\n",
    "for regime, count in regime_counts.items():\n",
    "    percentage = (count / len(regime_data)) * 100\n",
    "    print(f\"  " {regime}: {count} points ({percentage:.1f}%)\")\n",
    "\n",
    "# Visualize regimes\n",
    "fig, (ax1, ax2, ax3) = plt.subplots(3, 1, figsize=(15, 12))\n",
    "fig.suptitle('<
 FluidTrade: Market Regime Detection', fontsize=16, fontweight='bold')\n",
    "\n",
    "# Focus on a subset for clarity\n",
    "subset = slice(0, 3000)\n",
    "t_subset = regime_data['timestamp'].iloc[subset]\n",
    "price_subset = regime_data['price'].iloc[subset]\n",
    "regime_subset = regime_data['regime'].iloc[subset]\n",
    "turbulence_subset = regime_data['turbulence_score'].iloc[subset]\n",
    "\n",
    "# Plot 1: Price with regime overlay\n",
    "regime_colors = {'Laminar': 'green', 'Turbulent': 'red', 'Transition': 'orange', 'Unknown': 'gray'}\n",
    "for regime in regime_colors.keys():\n",
    "    mask = regime_subset == regime\n",
