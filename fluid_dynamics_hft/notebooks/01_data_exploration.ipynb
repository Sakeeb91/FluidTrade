    "def transform_market_to_fluid(data, window_size=50):\n",
    "    \"\"\"\n",
    "    Transform market data into fluid dynamics variables.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    data : pd.DataFrame\n",
    "        Market data with columns: price, volume, order_flow, spread\n",
    "    window_size : int\n",
    "        Window size for calculating derivatives and smoothing\n",
    "    \n",
    "    Returns:\n",
    "    --------\n",
    "    pd.DataFrame with fluid variables: u, v, pressure, density, vorticity\n",
    "    \"\"\"\n",
    "    \n",
    "    fluid_data = data.copy()\n",
    "    \n",
    "    # 1. Velocity Field Components\n",
    "    # u-component: horizontal flow from order flow dynamics\n",
    "    volume_density = data['volume'] / data['volume'].max()\n",
    "    fluid_data['u_velocity'] = data['order_flow'] / (volume_density + 0.01)  # Avoid division by zero\n",
    "    \n",
    "    # v-component: vertical flow from price momentum\n",
    "    price_returns = data['price'].pct_change().fillna(0)\n",
    "    fluid_data['v_velocity'] = gaussian_filter1d(price_returns * 1000, sigma=2)  # Scaled and smoothed\n",
    "    \n",
    "    # 2. Pressure Field (inverse relationship with spread)\n",
    "    mean_spread = data['spread'].mean()\n",
    "    fluid_data['pressure'] = -np.log(data['spread'] / mean_spread)\n",
    "    \n",
    "    # 3. Density Field (normalized volume)\n",
    "    fluid_data['density'] = volume_density\n",
    "    \n",
    "    # 4. Calculate derivatives for vorticity\n",
    "    # Using finite differences with smoothing\n",
    "    dt = data['timestamp'].diff().fillna(data['timestamp'].iloc[1] - data['timestamp'].iloc[0])\n",
    "    \n",
    "    # Smooth velocity fields for better derivatives\n",
    "    u_smooth = gaussian_filter1d(fluid_data['u_velocity'], sigma=3)\n",
    "    v_smooth = gaussian_filter1d(fluid_data['v_velocity'], sigma=3)\n",
    "    \n",
    "    # Calculate vorticity (simplified 1D approximation)\n",
    "    du_dt = np.gradient(u_smooth)\n",
    "    dv_dt = np.gradient(v_smooth)\n",
    "    fluid_data['vorticity'] = dv_dt - du_dt  # Approximate vorticity\n",
    "    \n",
    "    # 5. Calculate additional fluid properties\n",
    "    # Velocity magnitude\n",
    "    fluid_data['velocity_magnitude'] = np.sqrt(u_smooth**2 + v_smooth**2)\n",
    "    \n",
    "    # Flow divergence (approximation)\n",
    "    fluid_data['divergence'] = np.gradient(u_smooth) + np.gradient(v_smooth)\n",
    "    \n",
    "    # Kinetic energy density\n",
    "    fluid_data['kinetic_energy'] = 0.5 * fluid_data['density'] * fluid_data['velocity_magnitude']**2\n",
    "    \n",
    "    return fluid_data\n",
    "\n",
    "# Transform market data to fluid variables\n",
    "print(\"<
 Transforming market data to fluid dynamics variables...\")\n",
    "fluid_data = transform_market_to_fluid(market_data)\n",
    "\n",
    "print(\" Transformation complete!\")\n",
    "print(\"\\n=, Fluid Variables Generated:\")\n",
    "fluid_vars = ['u_velocity', 'v_velocity', 'pressure', 'density', 'vorticity', 'velocity_magnitude', 'divergence', 'kinetic_energy']\n",
    "for var in fluid_vars:\n",
    "    print(f\"  " {var}: range [{fluid_data[var].min():.3f}, {fluid_data[var].max():.3f}]\")\n",
    "\n",
    "# Display fluid data sample\n",
    "print(\"\\n=Ê Sample Fluid Data:\")\n",
    "fluid_data[['timestamp', 'price'] + fluid_vars].head()"
   ]
