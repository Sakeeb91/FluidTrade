    "def detect_vortex_patterns(fluid_data, vorticity_threshold=1.5, min_duration=10):\n",
    "    \"\"\"\n",
    "    Detect vortex formations in market flow.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    fluid_data : pd.DataFrame\n",
    "        Data with fluid dynamics variables\n",
    "    vorticity_threshold : float\n",
    "        Minimum vorticity strength for detection\n",
    "    min_duration : int\n",
    "        Minimum duration for valid vortex\n",
    "    \n",
    "    Returns:\n",
    "    --------\n",
    "    list of dict: Detected vortex events\n",
    "    \"\"\"\n",
    "    \n",
    "    # Find high vorticity regions\n",
    "    high_vorticity = np.abs(fluid_data['vorticity']) > vorticity_threshold\n",
    "    \n",
    "    # Find continuous vortex regions\n",
    "    vortex_regions = []\n",
    "    in_vortex = False\n",
    "    start_idx = None\n",
    "    \n",
    "    for i, is_vortex in enumerate(high_vorticity):\n",
    "        if is_vortex and not in_vortex:\n",
    "            # Start of vortex\n",
    "            in_vortex = True\n",
    "            start_idx = i\n",
    "        elif not is_vortex and in_vortex:\n",
    "            # End of vortex\n",
    "            in_vortex = False\n",
    "            duration = i - start_idx\n",
    "            \n",
    "            if duration >= min_duration:\n",
    "                vortex_data = fluid_data.iloc[start_idx:i]\n",
    "                vortex_info = {\n",
    "                    'start_time': vortex_data['timestamp'].iloc[0],\n",
    "                    'end_time': vortex_data['timestamp'].iloc[-1],\n",
    "                    'duration': duration,\n",
    "                    'strength': np.abs(vortex_data['vorticity']).mean(),\n",
    "                    'direction': 'clockwise' if vortex_data['vorticity'].mean() > 0 else 'counterclockwise',\n",
    "                    'start_price': vortex_data['price'].iloc[0],\n",
    "                    'end_price': vortex_data['price'].iloc[-1],\n",
    "                    'price_change': vortex_data['price'].iloc[-1] - vortex_data['price'].iloc[0],\n",
    "                    'start_idx': start_idx,\n",
    "                    'end_idx': i-1\n",
    "                }\n",
    "                vortex_regions.append(vortex_info)\n",
    "    \n",
    "    return vortex_regions\n",
