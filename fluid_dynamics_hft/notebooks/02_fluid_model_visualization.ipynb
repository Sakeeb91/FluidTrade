    "    return fig\n",
    "\n",
    "def create_interactive_dashboard(fluid_data):\n",
    "    \"\"\"\n",
    "    Create an interactive trading dashboard.\n",
    "    \"\"\"\n",
    "    # Sample subset for dashboard\n",
    "    subset = fluid_data.iloc[::10].copy()  # Sample every 10th point for performance\n",
    "    \n",
    "    # Create dashboard with multiple panels\n",
    "    fig = make_subplots(\n",
    "        rows=3, cols=2,\n",
    "        subplot_titles=(\n",
    "            'Price with Regime Detection', 'Velocity Components',\n",
    "            'Pressure vs Volume', 'Vorticity Detection',\n",
    "            'Energy Evolution', 'Reynolds Number'\n",
    "        ),\n",
    "        specs=[\n",
    "            [{'secondary_y': True}, {}],\n",
    "            [{}, {}],\n",
    "            [{}, {}]\n",
    "        ]\n",
    "    )\n",
