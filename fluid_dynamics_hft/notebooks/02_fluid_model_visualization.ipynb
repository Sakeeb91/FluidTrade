    "def create_flow_field_visualization(fluid_data, time_start=0, time_end=5000, grid_resolution=50):\n",
    "    \"\"\"\n",
    "    Create beautiful flow field visualization with streamlines.\n",
    "    \"\"\"\n",
    "    # Extract subset for visualization\n",
    "    subset = fluid_data.iloc[time_start:time_end].copy()\n",
    "    \n",
    "    # Create a grid for flow field\n",
    "    t_min, t_max = subset['timestamp'].min(), subset['timestamp'].max()\n",
    "    p_min, p_max = subset['price'].min(), subset['price'].max()\n",
    "    \n",
    "    # Extend price range slightly for better visualization\n",
    "    p_range = p_max - p_min\n",
    "    p_min -= 0.1 * p_range\n",
    "    p_max += 0.1 * p_range\n",
    "    \n",
    "    t_grid = np.linspace(t_min, t_max, grid_resolution)\n",
    "    p_grid = np.linspace(p_min, p_max, grid_resolution)\n",
    "    T_grid, P_grid = np.meshgrid(t_grid, p_grid)\n",
    "    \n",
    "    # Interpolate velocity components onto grid\n",
    "    points = np.column_stack((subset['timestamp'], subset['price']))\n",
    "    \n",
    "    u_interp = griddata(points, subset['u_velocity'], (T_grid, P_grid), method='linear', fill_value=0)\n",
    "    v_interp = griddata(points, subset['v_velocity'], (T_grid, P_grid), method='linear', fill_value=0)\n",
    "    pressure_interp = griddata(points, subset['pressure'], (T_grid, P_grid), method='linear', fill_value=0)\n",
    "    \n",
    "    # Create the visualization\n",
    "    fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
    "    fig.suptitle('<
 FluidTrade: Market Flow Field Analysis', fontsize=16, fontweight='bold')\n",
    "    \n",
    "    # Plot 1: Streamlines with pressure background\n",
    "    ax1 = axes[0, 0]\n",
    "    \n",
    "    # Background pressure field\n",
    "    pressure_plot = ax1.contourf(T_grid, P_grid, pressure_interp, levels=20, cmap='RdYlBu', alpha=0.7)\n",
    "    plt.colorbar(pressure_plot, ax=ax1, label='Pressure')\n",
    "    \n",
    "    # Streamlines\n",
    "    ax1.streamplot(T_grid, P_grid, u_interp, v_interp, \n",
    "                   color='black', density=1.5, linewidth=1, arrowsize=1.2)\n",
    "    \n",
    "    # Overlay actual price path\n",
    "    ax1.plot(subset['timestamp'], subset['price'], 'white', linewidth=2, alpha=0.8, label='Price Path')\n",
    "    \n",
    "    ax1.set_title('<
 Flow Streamlines with Pressure Field')\n",
    "    ax1.set_xlabel('Time (seconds)')\n",
    "    ax1.set_ylabel('Price ($)')\n",
    "    ax1.legend()\n",
    "    \n",
    "    # Plot 2: Velocity magnitude field\n",
    "    ax2 = axes[0, 1]\n",
    "    \n",
    "    velocity_magnitude = np.sqrt(u_interp**2 + v_interp**2)\n",
    "    velocity_plot = ax2.contourf(T_grid, P_grid, velocity_magnitude, levels=20, cmap='plasma')\n",
    "    plt.colorbar(velocity_plot, ax=ax2, label='Flow Speed')\n",
    "    \n",
    "    # Add velocity vectors\n",
    "    skip = 3  # Show every 3rd vector for clarity\n",
    "    ax2.quiver(T_grid[::skip, ::skip], P_grid[::skip, ::skip], \n",
    "               u_interp[::skip, ::skip], v_interp[::skip, ::skip], \n",
    "               alpha=0.6, scale=None, scale_units='xy')\n",
    "    \n",
    "    ax2.plot(subset['timestamp'], subset['price'], 'white', linewidth=2, alpha=0.8)\n",
    "    ax2.set_title('=€ Flow Speed and Direction')\n",
    "    ax2.set_xlabel('Time (seconds)')\n",
    "    ax2.set_ylabel('Price ($)')\n",
    "    \n",
    "    # Plot 3: Vorticity field\n",
    "    ax3 = axes[1, 0]\n",
    "    \n",
    "    vorticity_interp = griddata(points, subset['vorticity'], (T_grid, P_grid), method='linear', fill_value=0)\n",
    "    vorticity_plot = ax3.contourf(T_grid, P_grid, vorticity_interp, levels=20, cmap='RdBu', \n",
    "                                  vmin=-np.abs(vorticity_interp).max(), vmax=np.abs(vorticity_interp).max())\n",
    "    plt.colorbar(vorticity_plot, ax=ax3, label='Vorticity')\n",
    "    \n",
    "    ax3.plot(subset['timestamp'], subset['price'], 'black', linewidth=2, alpha=0.8)\n",
    "    ax3.set_title('<* Vorticity Field (Red: CW, Blue: CCW)')\n",
    "    ax3.set_xlabel('Time (seconds)')\n",
    "    ax3.set_ylabel('Price ($)')\n",
    "    \n",
    "    # Plot 4: Energy density with regime overlay\n",
    "    ax4 = axes[1, 1]\n",
    "    \n",
    "    # Color code by regime\n",
    "    regime_colors = {'trending_up': 'green', 'volatile': 'red', 'trending_down': 'orange', \n",
    "                     'sideways': 'blue', 'breakout': 'purple'}\n",
    "    \n",
    "    for regime in subset['regime_label'].unique():\n",
    "        regime_mask = subset['regime_label'] == regime\n",
    "        regime_subset = subset[regime_mask]\n",
    "        ax4.scatter(regime_subset['timestamp'], regime_subset['kinetic_energy'], \n",
    "                   c=regime_colors[regime], label=regime.replace('_', ' ').title(), \n",
    "                   alpha=0.7, s=10)\n",
    "    \n",
    "    ax4.set_title('¡ Kinetic Energy by Market Regime')\n",
    "    ax4.set_xlabel('Time (seconds)')\n",
    "    ax4.set_ylabel('Energy Density')\n",
    "    ax4.legend(bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "    ax4.grid(True, alpha=0.3)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    return T_grid, P_grid, u_interp, v_interp, pressure_interp\n",
    "\n",
    "# Create flow field visualization\n",
    "print(\"<¨ Creating advanced flow field visualization...\")\n",
    "T_grid, P_grid, u_interp, v_interp, pressure_interp = create_flow_field_visualization(\n",
    "    fluid_data, time_start=2000, time_end=8000\n",
    ")\n",
    "print(\" Flow field visualization complete!\")"
