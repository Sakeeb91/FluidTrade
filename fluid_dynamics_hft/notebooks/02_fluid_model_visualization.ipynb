    "def create_3d_market_surface(fluid_data, time_start=1000, time_end=4000):\n",
    "    \"\"\"\n",
    "    Create interactive 3D surface plot of market pressure.\n",
    "    \"\"\"\n",
    "    subset = fluid_data.iloc[time_start:time_end].copy()\n",
    "    \n",
    "    # Create a finer grid for smooth 3D surface\n",
    "    t_min, t_max = subset['timestamp'].min(), subset['timestamp'].max()\n",
    "    p_min, p_max = subset['price'].min(), subset['price'].max()\n",
    "    \n",
    "    # Create regular grid\n",
    "    t_grid = np.linspace(t_min, t_max, 60)\n",
    "    p_grid = np.linspace(p_min, p_max, 60)\n",
    "    T_3d, P_3d = np.meshgrid(t_grid, p_grid)\n",
    "    \n",
    "    # Interpolate pressure and other variables onto grid\n",
    "    points = np.column_stack((subset['timestamp'], subset['price']))\n",
    "    pressure_3d = griddata(points, subset['pressure'], (T_3d, P_3d), method='cubic', fill_value=0)\n",
    "    vorticity_3d = griddata(points, subset['vorticity'], (T_3d, P_3d), method='cubic', fill_value=0)\n",
    "    energy_3d = griddata(points, subset['kinetic_energy'], (T_3d, P_3d), method='cubic', fill_value=0)\n",
    "    \n",
    "    # Create subplots for multiple 3D views\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=2,\n",
    "        subplot_titles=('Pressure Surface', 'Vorticity Landscape', 'Energy Terrain', 'Price Path 3D'),\n",
    "        specs=[[{'type': 'surface'}, {'type': 'surface'}],\n",
    "               [{'type': 'surface'}, {'type': 'scatter3d'}]]\n",
    "    )\n",
    "    \n",
    "    # Plot 1: Pressure surface\n",
    "    fig.add_trace(\n",
    "        go.Surface(\n",
    "            x=T_3d, y=P_3d, z=pressure_3d,\n",
    "            colorscale='RdYlBu',\n",
    "            name='Pressure',\n",
    "            showscale=True\n",
    "        ),\n",
    "        row=1, col=1\n",
    "    )\n",
    "    \n",
    "    # Plot 2: Vorticity surface\n",
    "    fig.add_trace(\n",
    "        go.Surface(\n",
    "            x=T_3d, y=P_3d, z=vorticity_3d,\n",
    "            colorscale='RdBu',\n",
    "            name='Vorticity',\n",
    "            showscale=True\n",
    "        ),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # Plot 3: Energy surface\n",
    "    fig.add_trace(\n",
    "        go.Surface(\n",
    "            x=T_3d, y=P_3d, z=energy_3d,\n",
    "            colorscale='Plasma',\n",
    "            name='Energy',\n",
    "            showscale=True\n",
    "        ),\n",
    "        row=2, col=1\n",
    "    )\n",
    "    \n",
    "    # Plot 4: 3D price path with regime coloring\n",
    "    regime_colors_3d = {'trending_up': 'green', 'volatile': 'red', 'trending_down': 'orange', \n",
    "                         'sideways': 'blue', 'breakout': 'purple'}\n",
    "    \n",
    "    for regime in subset['regime_label'].unique():\n",
    "        regime_mask = subset['regime_label'] == regime\n",
    "        regime_subset = subset[regime_mask]\n",
    "        \n",
    "        fig.add_trace(\n",
    "            go.Scatter3d(\n",
    "                x=regime_subset['timestamp'],\n",
    "                y=regime_subset['price'],\n",
    "                z=regime_subset['pressure'],\n",
    "                mode='markers+lines',\n",
    "                marker=dict(color=regime_colors_3d[regime], size=3),\n",
    "                line=dict(color=regime_colors_3d[regime], width=2),\n",
    "                name=regime.replace('_', ' ').title()\n",
