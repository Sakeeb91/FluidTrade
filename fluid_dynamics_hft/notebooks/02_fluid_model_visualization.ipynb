    "    fig.suptitle('<* FluidTrade: Animated Vorticity Evolution', fontsize=14, fontweight='bold')\n",
    "    \n",
    "    def animate(frame):\n",
    "        ax1.clear()\n",
    "        ax2.clear()\n",
    "        \n",
    "        # Calculate time window for this frame\n",
    "        start_idx = frame * step_size\n",
    "        end_idx = start_idx + time_window\n",
    "        \n",
    "        if end_idx > len(fluid_data):\n",
    "            return\n",
    "        \n",
    "        subset = fluid_data.iloc[start_idx:end_idx]\n",
    "        \n",
    "        # Plot 1: Current price with vorticity overlay\n",
    "        ax1.plot(subset['timestamp'], subset['price'], 'b-', linewidth=2, label='Price')\n",
    "        \n",
    "        # Color-code by vorticity strength\n",
    "        vorticity_colors = subset['vorticity']\n",
    "        scatter = ax1.scatter(subset['timestamp'], subset['price'], \n",
    "                             c=vorticity_colors, cmap='RdBu', \n",
    "                             s=np.abs(vorticity_colors)*20 + 10,\n",
    "                             alpha=0.7, edgecolors='black', linewidth=0.5)\n",
    "        \n",
    "        ax1.set_title(f'Price Path with Vorticity (t={subset[\"timestamp\"].iloc[0]:.1f}s - {subset[\"timestamp\"].iloc[-1]:.1f}s)')\n",
    "        ax1.set_xlabel('Time (seconds)')\n",
    "        ax1.set_ylabel('Price ($)')\n",
    "        ax1.grid(True, alpha=0.3)\n",
    "        ax1.legend()\n",
    "        \n",
    "        # Add colorbar for vorticity\n",
    "        plt.colorbar(scatter, ax=ax1, label='Vorticity', shrink=0.8)\n",
    "        \n",
    "        # Plot 2: Vorticity time series with detection\n",
    "        ax2.plot(subset['timestamp'], subset['vorticity'], 'g-', linewidth=2)\n",
    "        ax2.axhline(y=1.0, color='red', linestyle='--', alpha=0.7, label='Vortex Threshold')\n",
    "        ax2.axhline(y=-1.0, color='red', linestyle='--', alpha=0.7)\n",
