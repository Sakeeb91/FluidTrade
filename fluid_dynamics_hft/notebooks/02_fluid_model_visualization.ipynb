    "        if vortex_mask.any():\n",
    "            ax2.scatter(subset['timestamp'][vortex_mask], subset['vorticity'][vortex_mask], \n",
    "                       c='red', s=50, alpha=0.8, label='Vortex Detected', zorder=10)\n",
    "        \n",
    "        ax2.set_title('Vorticity Evolution with Detection')\n",
    "        ax2.set_xlabel('Time (seconds)')\n",
    "        ax2.set_ylabel('Vorticity')\n",
    "        ax2.grid(True, alpha=0.3)\n",
    "        ax2.legend()\n",
    "        \n",
