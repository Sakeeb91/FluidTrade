    "def generate_enhanced_market_data(n_points=15000, dt=0.001, base_price=100.0):\n",
    "    \"\"\"\n",
    "    Generate enhanced synthetic market data with multiple regimes and realistic patterns.\n",
    "    \"\"\"\n",
    "    times = np.arange(n_points) * dt\n",
    "    \n",
    "    # Create multiple market regimes\n",
    "    regime_changes = [0, 3000, 6000, 9000, 12000, 15000]\n",
    "    regimes = ['trending_up', 'volatile', 'trending_down', 'sideways', 'breakout']\n",
    "    \n",
    "    prices = []\n",
    "    volumes = []\n",
    "    order_flows = []\n",
    "    \n",
    "    current_price = base_price\n",
    "    \n",
    "    for i in range(len(regime_changes)-1):\n",
    "        start_idx = regime_changes[i]\n",
    "        end_idx = regime_changes[i+1]\n",
    "        regime_length = end_idx - start_idx\n",
    "        regime = regimes[i]\n",
    "        \n",
    "        regime_times = times[start_idx:end_idx]\n",
    "        \n",
    "        if regime == 'trending_up':\n",
    "            trend = 0.05 * np.ones(regime_length)\n",
    "            noise_scale = 0.01\n",
    "            volume_base = 1200\n",
    "        elif regime == 'volatile':\n",
    "            trend = 0.02 * np.sin(4 * np.pi * regime_times / 10)\n",
    "            noise_scale = 0.03\n",
    "            volume_base = 1800\n",
    "        elif regime == 'trending_down':\n",
    "            trend = -0.03 * np.ones(regime_length)\n",
    "            noise_scale = 0.015\n",
    "            volume_base = 1400\n",
    "        elif regime == 'sideways':\n",
    "            trend = 0.005 * np.sin(8 * np.pi * regime_times / 20)\n",
    "            noise_scale = 0.008\n",
    "            volume_base = 800\n",
    "        else:  # breakout\n",
    "            trend = 0.08 * np.tanh(2 * (regime_times - regime_times[0] - 1))\n",
    "            noise_scale = 0.02\n",
    "            volume_base = 2000\n",
    "        \n",
    "        # Generate price movements for this regime\n",
    "        noise = noise_scale * np.random.randn(regime_length)\n",
    "        regime_returns = (trend + noise) * dt\n",
    "        regime_prices = current_price * np.exp(np.cumsum(regime_returns))\n",
    "        \n",
    "        # Generate volume with regime-specific characteristics\n",
    "        regime_volumes = volume_base * (1 + 0.5 * np.random.exponential(1, regime_length))\n",
    "        regime_volumes *= (1 + 2 * np.abs(np.diff(np.concatenate([[regime_prices[0]], regime_prices]))))\n",
    "        \n",
    "        # Generate order flow with momentum effects\n",
    "        price_momentum = np.diff(np.concatenate([[regime_prices[0]], regime_prices]))\n",
    "        regime_order_flow = np.sign(price_momentum) * regime_volumes + 150 * np.random.randn(regime_length)\n",
    "        \n",
    "        prices.extend(regime_prices)\n",
    "        volumes.extend(regime_volumes)\n",
    "        order_flows.extend(regime_order_flow)\n",
    "        \n",
    "        current_price = regime_prices[-1]\n",
    "    \n",
    "    # Convert to arrays\n",
    "    prices = np.array(prices)\n",
    "    volumes = np.array(volumes)\n",
    "    order_flows = np.array(order_flows)\n",
    "    \n",
    "    # Calculate bid-ask spreads\n",
    "    volatility = np.abs(np.diff(np.concatenate([[prices[0]], prices]))) * 100\n",
    "    spreads = 0.01 + 0.03 * volatility\n",
    "    bids = prices - spreads/2\n",
    "    asks = prices + spreads/2\n",
    "    \n",
    "    # Create enhanced DataFrame with regime labels\n",
    "    regime_labels = []\n",
    "    for i, (start, end) in enumerate(zip(regime_changes[:-1], regime_changes[1:])):\n",
    "        regime_labels.extend([regimes[i]] * (end - start))\n",
    "    \n",
    "    data = pd.DataFrame({\n",
    "        'timestamp': times,\n",
    "        'price': prices,\n",
    "        'bid': bids,\n",
    "        'ask': asks,\n",
    "        'volume': volumes,\n",
    "        'order_flow': order_flows,\n",
    "        'spread': spreads,\n",
    "        'regime_label': regime_labels\n",
    "    })\n",
    "    \n",
    "    return data\n",
    "\n",
    "def enhanced_market_to_fluid_transform(data):\n",
    "    \"\"\"\n",
    "    Enhanced transformation with additional fluid properties.\n",
    "    \"\"\"\n",
    "    fluid_data = data.copy()\n",
    "    \n",
    "    # Basic transformations\n",
    "    volume_density = data['volume'] / data['volume'].max()\n",
    "    fluid_data['u_velocity'] = data['order_flow'] / (volume_density + 0.01)\n",
    "    \n",
    "    price_returns = data['price'].pct_change().fillna(0)\n",
    "    fluid_data['v_velocity'] = gaussian_filter1d(price_returns * 1000, sigma=2)\n",
    "    \n",
    "    mean_spread = data['spread'].mean()\n",
    "    fluid_data['pressure'] = -np.log(data['spread'] / mean_spread)\n",
    "    fluid_data['density'] = volume_density\n",
    "    \n",
    "    # Enhanced calculations\n",
    "    u_smooth = gaussian_filter1d(fluid_data['u_velocity'], sigma=3)\n",
    "    v_smooth = gaussian_filter1d(fluid_data['v_velocity'], sigma=3)\n",
    "    \n",
    "    fluid_data['vorticity'] = np.gradient(v_smooth) - np.gradient(u_smooth)\n",
    "    fluid_data['velocity_magnitude'] = np.sqrt(u_smooth**2 + v_smooth**2)\n",
    "    fluid_data['divergence'] = np.gradient(u_smooth) + np.gradient(v_smooth)\n",
    "    fluid_data['kinetic_energy'] = 0.5 * fluid_data['density'] * fluid_data['velocity_magnitude']**2\n",
    "    \n",
    "    # Additional fluid properties\n",
    "    fluid_data['reynolds_number'] = fluid_data['velocity_magnitude'] * fluid_data['density'] / 0.01  # Rough approximation\n",
    "    fluid_data['pressure_gradient'] = np.gradient(fluid_data['pressure'])\n",
    "    fluid_data['enstrophy'] = 0.5 * fluid_data['vorticity']**2  # Vorticity intensity\n",
    "    \n",
    "    return fluid_data\n",
    "\n",
    "# Generate enhanced dataset\n",
    "print(\"= Generating enhanced multi-regime market data...\")\n",
    "market_data = generate_enhanced_market_data(n_points=15000)\n",
    "fluid_data = enhanced_market_to_fluid_transform(market_data)\n",
    "\n",
    "print(f\" Generated {len(fluid_data)} data points across 5 market regimes\")\n",
    "print(f\"=Ê Regimes: {', '.join(fluid_data['regime_label'].unique())}\")\n",
    "print(f\"=° Price range: ${fluid_data['price'].min():.2f} to ${fluid_data['price'].max():.2f}\")\n",
    "\n",
    "# Show regime distribution\n",
    "regime_counts = fluid_data['regime_label'].value_counts()\n",
    "print(\"\\n=È Regime Distribution:\")\n",
    "for regime, count in regime_counts.items():\n",
    "    print(f\"  " {regime}: {count} points ({count/len(fluid_data)*100:.1f}%)\")"
   ]
