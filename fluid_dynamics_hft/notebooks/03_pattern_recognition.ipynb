    "    \n",
    "    # Plot 3: Fluid dynamics anomalies\n",
    "    ax3 = axes[1, 0]\n",
    "    \n",
    "    # Plot vorticity and energy\n",
    "    ax3.plot(subset['timestamp'], subset['vorticity'], 'b-', linewidth=1, label='Vorticity')\n",
    "    ax3_twin = ax3.twinx()\n",
