    "    ax3_twin.set_ylabel('Energy', color='red')\n",
    "    ax3.tick_params(axis='y', labelcolor='blue')\n",
    "    ax3_twin.tick_params(axis='y', labelcolor='red')\n",
    "    ax3.grid(True, alpha=0.3)\n",
    "    \n",
    "    # Plot 4: Anomaly frequency over time\n",
    "    ax4 = axes[1, 1]\n",
    "    \n",
